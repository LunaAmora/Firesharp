true  : -> bool : 1 #bool end
false : -> bool : 0 #bool end
null  : -> ptr  : 0 #ptr end
!bool : bool ptr : !32 end
!ptr  : ptr  ptr : !32 end
!int  : int  ptr : !32 end
@bool : ptr -> bool : @32 #bool end
@ptr  : ptr -> ptr  : @32 #ptr end
@int  : ptr -> int  : @32 end

+ptr : int ptr -> ptr : swap #ptr + end
ptr+ : ptr int -> ptr : #ptr + end
ptr- : ptr int -> ptr : #ptr - end

puts : int ptr :
  ciovec: 8 end
  written: 4 end
  ciovec !ptr
  ciovec 4 ptr+ !int
  1 ciovec 1 written fd_write drop
end

